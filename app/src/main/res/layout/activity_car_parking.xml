<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/images"
    tools:context="com.example.hp.carparkingbangalore.CarParking"
    android:id="@+id/line"



    android:weightSum="1">
    android:orientation="horizontal"
    <!--android:weightSum="1"-->


    <Button
        android:id="@+id/buttondisplay"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_x="53dp"
        android:layout_y="458dp"
        android:text="History" />

    <Button
        android:id="@+id/buttonclear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="229dp"
        android:layout_y="458dp"
        android:text="Reset" />

    <Button
        android:id="@+id/button1"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="35dp"
        android:layout_y="130dp"
        android:layout_below="@+id/button3"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="123dp"

        android:text="Slot1"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="35dp"
        android:layout_y="195dp"
        android:text="Slot3" />

    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="35dp"
        android:layout_y="260dp"
        android:text="Slot5" />

    <Button
        android:id="@+id/button7"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="35dp"
        android:layout_y="325dp"
        android:text="Slot7" />

    <Button
        android:id="@+id/button9"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="35dp"
        android:layout_y="390dp"
        android:text="Slot9" />

    <Button
        android:id="@+id/button2"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="240dp"
        android:layout_alignStart="@+id/button"
        android:layout_below="@+id/text3"
        android:layout_marginStart="26dp"
        android:layout_marginTop="51dp"

        android:layout_y="130dp"
        android:text="Slot2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="240dp"
        android:layout_y="195dp"
        android:text="Slot4" />

    <Button
        android:id="@+id/button6"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="240dp"
        android:layout_y="260dp"
        android:text="Slot6" />

    <Button
        android:id="@+id/button8"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="240dp"
        android:layout_y="325dp"
        android:text="Slot8" />

    <Button
        android:id="@+id/button10"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_x="240dp"
        android:layout_y="390dp"
        android:text="Slot10" />

    <Button
        android:id="@+id/button"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="68dp"
        android:layout_marginTop="12dp"
        android:layout_toEndOf="@+id/text3"
        android:layout_x="247dp"
        android:layout_y="73dp"
        android:text="Submit" />

    <EditText
        android:id="@+id/contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="79dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="contact no..." />

    <EditText
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="18dp"
        android:layout_y="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="enter name..." />

    <EditText
        android:id="@+id/text3"
        android:layout_width="232dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button2"
        android:layout_alignStart="@+id/button3"
        android:layout_marginStart="39dp"
        android:layout_x="18dp"
        android:layout_y="2dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="enter car no..." />

</AbsoluteLayout>
